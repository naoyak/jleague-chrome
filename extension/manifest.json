{
  "manifest_version": 2,

  "name": "J-League Translator",
  "description": "This extension translates J-League box score names.",
  "version": "1.0",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "browser_action": {
  },
  "content_scripts": [
  {
    "matches": ["http://www.j-league.or.jp/live/*"],
    "js": ["jquery.js", "jleague.js"]
  }],
  
  "permissions": [
    "http://www.j-league.or.jp/*"
  ]

}
